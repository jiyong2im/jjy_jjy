<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e7e8ea  "
    tools:context=".join">


    <Button
        android:id="@+id/btn_join"
        android:layout_width="270dp"
        android:layout_height="72dp"
        android:layout_marginBottom="44dp"
        android:background="@drawable/button_btn"
        android:fontFamily="@font/bmhanna_11yrs_otf"
        android:text="가입하기"
        android:textColor="#ffffff"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/edit_Cid"
        android:layout_width="410dp"
        android:layout_height="57dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="532dp"
        android:background="@drawable/edittext_background"
        android:backgroundTint="#ffffff"
        android:ems="10"
        android:fontFamily="@font/bmhannaair_otf"
        android:hint="  이름"
        android:inputType="textPersonName"
        android:maxLength="15"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edit_text_pw"
        android:layout_width="410dp"
        android:layout_height="57dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="292dp"
        android:background="@drawable/edittext_background"
        android:backgroundTint="#ffffff"
        android:ems="10"
        android:fontFamily="@font/bmhannaair_otf"
        android:hint="  **********"
        android:inputType="textPassword"
        android:maxLength="20"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/bmhanna_11yrs_otf"
        android:text="비밀번호"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/edit_text_pw"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.984" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/bmhanna_11yrs_otf"
        android:text="이름"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/edit_Cid"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/edit_pw_re"
        android:layout_width="410dp"
        android:layout_height="57dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="416dp"
        android:background="@drawable/edittext_background"
        android:backgroundTint="#ffffff"
        android:ems="10"
        android:fontFamily="@font/bmhannaair_otf"
        android:hint="  **********"
        android:inputType="textPassword"
        android:maxLength="15"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/bmhanna_11yrs_otf"
        android:text="비밀번호 확인"
        android:textSize="21sp"
        app:layout_constraintBottom_toTopOf="@+id/edit_pw_re"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/edit_phone"
        android:layout_width="410dp"
        android:layout_height="57dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="788dp"
        android:background="@drawable/edittext_background"
        android:backgroundTint="#ffffff"
        android:ems="10"
        android:fontFamily="@font/bmhannaair_otf"
        android:hint="  123-4567-9393"
        android:inputType="phone"
        android:maxLength="14"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_Confirm"
        android:layout_width="113dp"
        android:layout_height="55dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/button_btn"
        android:backgroundTint="#f86d4a "
        android:fontFamily="@font/bmhanna_11yrs_otf"
        android:text="인증"
        android:textColor="#ffffff"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toEndOf="@+id/editTextTextPersonName"
        app:layout_constraintTop_toBottomOf="@+id/edit_phone" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/bmhanna_11yrs_otf"
        android:text="휴대전화"
        android:textSize="21sp"
        app:layout_constraintBottom_toTopOf="@+id/edit_phone"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="310dp"
        android:layout_height="57dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/edittext_background"
        android:backgroundTint="#ffffff"
        android:ems="10"
        android:fontFamily="@font/bmhannaair_otf"
        android:hint="  인증번호"
        android:inputType="textPersonName"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_phone" />

    <EditText
        android:id="@+id/edit_num"
        android:layout_width="215dp"
        android:layout_height="64dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="656dp"
        android:background="@drawable/edittext_background"
        android:backgroundTint="#ffffff"
        android:ems="10"
        android:fontFamily="@font/bmhannaair_otf"
        android:hint="  6자리"
        android:inputType="textPersonName"
        android:maxLength="6"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edit_num2"
        android:layout_width="186dp"
        android:layout_height="63dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="656dp"
        android:background="@drawable/edittext_background"
        android:backgroundTint="#ffffff"
        android:ems="10"
        android:fontFamily="@font/bmhannaair_otf"
        android:hint="  7자리"
        android:inputType="textPersonName"
        android:maxLength="6"
        android:textSize="22sp"
        app:layout_constraintStart_toEndOf="@+id/edit_num"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/bmhanna_11yrs_otf"
        android:text="주민등록번호"
        android:textSize="21sp"
        app:layout_constraintBottom_toTopOf="@+id/edit_num"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_overlap"
        android:layout_width="97dp"
        android:layout_height="62dp"
        android:layout_marginTop="164dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/button_btn"
        android:backgroundTint="#f86d4a "
        android:fontFamily="@font/bmhanna_11yrs_otf"
        android:rotationY="0"
        android:text="중복체크"
        android:textColor="#ffffff"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/edit_join_id"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_user"
        android:layout_width="140dp"
        android:layout_height="55dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="44dp"
        android:backgroundTint="#f86d4a "
        android:fontFamily="@font/bmhanna_11yrs_otf"
        android:text="일반가입"
        android:textColor="#ffffff"
        android:textSize="19sp"
        app:layout_constraintEnd_toStartOf="@+id/btn_owner"
        app:layout_constraintHorizontal_bias="0.923"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_owner"
        android:layout_width="149dp"
        android:layout_height="57dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="84dp"
        android:backgroundTint="#cccccc"
        android:fontFamily="@font/bmhanna_11yrs_otf"
        android:text="점주 가입"
        android:textColor="#ffffff"
        android:textSize="19sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <EditText
        android:id="@+id/edit_join_id"
        android:layout_width="340dp"
        android:layout_height="57dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="164dp"
        android:background="@drawable/edittext_background"
        android:backgroundTint="#ffffff"
        android:ems="10"
        android:fontFamily="@font/bmhannaair_otf"
        android:hint="  jjy"
        android:inputType="textPersonName"
        android:maxLength="15"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/bmhanna_11yrs_otf"
        android:text="아이디"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/edit_join_id"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>